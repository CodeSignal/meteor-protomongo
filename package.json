{
  "name": "@codesignal/meteor-protomongo",
  "version": "2.0.0",
  "description": "Extends meteor/mongo with handy async methods",
  "main": "lib/mongo.js",
  "author": "CodeSignal",
  "scripts": {
    "flow": "flow",
    "eslint": "eslint --max-warnings 0 --ignore-path .gitignore --ext .js --ext .jsx src/",
    "build": "npm run build:clean && npm run build:lib && npm run build:flow",
    "prepublish": "npm run build",
    "build:clean": "rimraf lib",
    "build:lib": "babel src -d lib",
    "build:flow": "flow-copy-source -v src lib"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-spread": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "flow": "^0.2.3",
    "flow-bin": "^0.186.0",
    "flow-copy-source": "^2.0.9",
    "rimraf": "^3.0.2"
  },
  "files": [
    "/lib"
  ],
  "license": "MIT",
  "homepage": "https://github.com/CodeSignal/meteor-protomongo",
  "keywords": [
    "meteor",
    "mongo",
    "async"
  ]
}
